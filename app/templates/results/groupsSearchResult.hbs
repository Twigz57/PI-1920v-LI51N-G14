<h1>{{this.key}}</h1>
{{#each this.body}}
<hr>
<div class="card border-primary mb-3">
  <div class="card-header">
   Name: {{name}}
  </div>
  <div class="card-body">
     <p class="card-title">id: {{id}}</p>
    <p class="card-title">Description: {{description}}</p>
    <h5>Tv Shows on Group:</h5>
    {{#each this.TV_Shows}}
      <p class="card-title">Name: {{TV_Show}}</p>
      <p class="card-title">Id:{{id}}</p>
     {{/each}}
  </div>
</div>
{{/each}}